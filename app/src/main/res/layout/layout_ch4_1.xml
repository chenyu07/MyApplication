<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:id="@+id/ch4_1_11"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="button1 " />

    <ImageView
        android:layout_width="200dp"
        android:id="@+id/ch4_iv"
        android:src="@raw/timg"
        android:layout_height="150dp" />
    <EditText
        android:layout_width="match_parent"
        android:id="@+id/ch4_1_mail"
        android:hint="plase input e_mail"
        android:layout_height="67dp" />
    <EditText
        android:layout_width="match_parent"
        android:id="@+id/ch4_1_tel"
        android:hint="plase input tel"
        android:layout_height="47dp" />
    <TextView
        android:layout_width="match_parent"
        android:id="@+id/ch4_1_tv"
        android:layout_height="82dp" />
    <Button
        android:layout_width="match_parent"
        android:text="MainActivity"
        android:onClick="startMain"
        android:layout_height="35dp" />


</LinearLayout>
